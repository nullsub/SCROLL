//directed role graph
Graph ::= Edge* Node*;
Edge ::= <Source: Object> <Target: Object>;
Node ::= <Value: Object>;

//role tree structure
Tree ::= Natural* DispatchQuery;
DispatchQuery ;
abstract Player ::= Role* <Object:Object> ;
Natural:Player ;
Role:Player ;

rel DispatchQuery.From*     -> Player;
rel DispatchQuery.To*       -> Player;
rel DispatchQuery.Through*  -> Player;
rel DispatchQuery.Bypassing* -> Player;
